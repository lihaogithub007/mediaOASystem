@CHARSET "UTF-8";
#print_btn{
	margin:0 auto;
	text-align:right;
	width:960px;
	padding:15px 56px 0px 0px;
}
#print_btn a{
	text-decoration: none;
	color: #3c3d5d;
	font-size:15px;
}
.print_area_main{
	width:960px;
	margin:0px auto;
	padding:5px; 
	font-family:"microsoft yahei"; 
	color:#000; 
	font-size:13px; 
	text-align:center;
	background-color: #fff !important;
}
.align_left{
	text-align: left;
}
.align_center{
	text-align: center;
}
.print_area_main .print_title{
	margin:0 auto;
	font-size:24px;
	font-weight: bold;
	padding:10px 0px 20px;
}
.print_area_main .print_table {   
    border-collapse: collapse;   
    border: none;   
    margin:0 auto;
}   
.print_area_main .print_table td {   
    border:1px solid #000;
    padding:5px;   
    width:140px;
    text-align:left;
} 
.print_area_main .print_note{
	font-size:13px;
	padding:5px 0px 10px 100px;
	line-height:24px;
	text-align:left;
}
.print_area_main .print_footer{
	padding:5px 30px 10px 0px;
	line-height:24px;
	text-align:right;
}
.print_area_main .print_footer label, .print_area_main .print_footer div{
	display: inline-block;
	font-weight:bold;
	font-size:15px;
}
.print_area_main .print_footer div{
	width:120px;
	border-bottom:1px solid #000;
}
.td_title{
	background-color: #eee;
	font-weight: bold;
}
.print_area_main .print_table td.td_thin{
	width:15px;
	text-align: center;
	vertical-align: middle;
}
.print_area_main .print_table td.td_middle{
	width:40px;
	text-align: center;
	vertical-align: middle;
}
.print_area_main .print_table td.td_xlarge{
	width:80px;
	text-align: center;
	vertical-align: middle;
}
.print_area_main .print_table td.td_xxlarge{
	width:120px;
	text-align: center;
	vertical-align: middle;
}