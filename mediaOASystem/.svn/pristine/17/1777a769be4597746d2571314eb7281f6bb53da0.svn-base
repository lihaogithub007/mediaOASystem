<script type="text/javascript">
jQuery(document).ready(function(){
	jQuery(".fancybox-button-view").fancybox({
        groupAttr: 'data-rel',
        prevEffect: 'none',
        nextEffect: 'none',
        closeBtn: true,
        live : false,
        helpers: {
            title: {
                type: 'inside'
            }
        }
    });
});
</script>