    <script src="$!webPath/resources/js/scripts.js"></script>
	<script>
	    var count=2;
		var interval = window.setInterval(go,1000);
	  	function go(){
	    	count--;
	    	$("#timetip").html(count+'秒后自动跳转');
	    	if(count==0) {
	    		window.location.href="$!url";
	    		clearInterval(interval);	    	
	    	}
	  	}
	</script>
