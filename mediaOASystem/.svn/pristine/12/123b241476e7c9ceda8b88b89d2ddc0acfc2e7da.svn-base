#if($!userContractRenewalList && $!userContractRenewalList.size()>0)
  	#foreach($obj in $!userContractRenewalList)
		<div class="bd" id="contractrenewal_view_$!obj.id">
        	<div class="con edit">
            	<div class="e e2_base">
				    <label>续签合同时间</label><i>：</i>
				    <div>&nbsp;$!CommUtil.parseShortDateTime($!obj.renewal_sign_time)</div>
				</div>
				<div class="e e2_base">
				    <label>续签到期时间</label><i>：</i>
				    <div>&nbsp;$!CommUtil.parseShortDateTime($!obj.renewal_expiration_time)</div>
				</div>
				<div class="e e2_base">
				    <label>续签年限</label><i>：</i>
				    <div>&nbsp;$!{obj.renewal_contract_duration}&nbsp;(年)</div>
				</div>
               	<div class="clearfix"></div>
               	<span class="ed_icon"><em class="edi icons" id="contractrenewal_edit_$!obj.id" onclick="editButtonClick(this)"></em>
				<em class="del icons" id="contractrenewal_delete_$!obj.id" onclick="deleteButtonClick(this)"></em></span>
			</div>
		</div>
	#end
#else
	<div class="none icons" id="contractrenewal_empty">没有续签记录！</div>
#end