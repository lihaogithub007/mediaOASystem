	                    $("#" + sType).attr("class", aTypeInfo["viewclass"]);
	                }
	                if("contract"==sType && ""==p_oObject.id.split("_")[2] && data.indexOf("没有合同信息")<0){
	                	$("#" + sType + "_new").unbind();
	                	$("#" + sType + "_new").hide();
