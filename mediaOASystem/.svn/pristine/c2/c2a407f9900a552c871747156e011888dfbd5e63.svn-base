<link href="$!webPath/resources/css/custom/resume_print.css" rel="stylesheet" type="text/css" />
</head>
<body class="fixed-top">
	<div id="header" class="navbar navbar-inverse navbar-fixed-top">
		$!httpInclude.include("/common/_header.htm") 
	</div>
	<div id="container" class="row-fluid">
		<div id="sidebar" class="nav-collapse collapse">
			$!httpInclude.include("/common/_sidebar.htm?op=workload_statistics_manage") 
		</div>
		<div id="main-content">
			<div class="container-fluid">
				<div class="row-fluid">
					<div class="span12">
						<h3 class="page-title">工作量打印预览</h3>
						<ul class="breadcrumb">
							<li><a href="$!webPath/index.htm"><i class="icon-home"></i></a><span class="divider">&nbsp;</span></li>
					        <li><a href="javascript:void(0);">统计管理</a> <span class="divider">&nbsp;</span></li>
							<li><a href="$!webPath/admin/statistics/workload_statistics_print.htm?name=$!name&beginTime=$!beginTime&endTime=$!endTime">工作量打印预览</a><span class="divider-last">&nbsp;</span></li>
						</ul>
					</div>
				</div>
				<div class="row-fluid">
	                <div class="span12">
	                    <div class="widget">
	                    	<div id="print_btn">
	                    		<a href="javascript:void(0);" onclick="printdiv('print_area_id')"><img src="$!webPath/resources/img/custom/print.png" align="absmiddle"/>&nbsp;打印</a>
	                    	</div>
	                    	<div id="print_area_id">
	                    		<div class="print_area_main">
		                    		<div class="print_title">工作量统计</div>
		                    		<table class="print_table">
		                    			<tr>												
		                    				<td style="width:5%;">序号</td>
		                    				<td style="width:10%;">姓名</td>
											<td style="width:5%;">亮点</td>
											<td style="width:5%;">APP写</td>
											<td style="width:5%;">APP发</td>
											<td style="width:5%;">FB写</td>
											<td style="width:5%;">FB发</td>
											<td style="width:5%;">微博写</td>
											<td style="width:5%;">微博发</td>
											<td style="width:5%;">TWT写</td>
											<td style="width:5%;">TWT发</td>
											<td style="width:5%;min-width:60px;">上传YT+腾讯</td>
											<td style="width:5%;">tumblr发</td>
											<td style="width:5%;">微信</td>
											<td style="width:5%;">inst</td>
											<td style="width:5%;">Google+</td>
											<td style="width:5%;">pinterest</td>
											<td style="width:5%;">小结</td>
		                    			</tr>
		                    			#if($!objs && $!objs.size()>0)
		                					#foreach($obj in $!objs)
			                    			<tr>				
			                    				<td class="td_thin">$!{velocityCount}</td>
			                    				<td>$!{obj.name}</td>
			                                    <td>$!obj.bright_spot</td>
												<td>$!obj.app_write</td>
												<td>$!obj.app_send</td>
												<td>$!obj.fb_write</td>
												<td>$!obj.fb_send</td>
												<td>$!obj.weibo_write</td>
												<td>$!obj.weibo_send</td>
												<td>$!obj.twt_write</td>
												<td>$!obj.twt_send</td>
												<td>$!obj.upload_yt_or_tencent</td>
												<td>$!obj.tumblr_send</td>
												<td>$!obj.WeChat</td>
												<td>$!obj.inst</td>
												<td>$!obj.google</td>
												<td>$!obj.pinterest</td>
												<td>$!obj.total</td>
			                    			</tr>
			                    			#end
					                	#end
		                    		</table>
		                    		<div class="print_footer"></div>
		                    	</div>
	                    	</div>
	                    </div>
	                </div>
				</div>
			</div>
		</div>
	</div>
	<div id="footer">
		$!httpInclude.include("/common/_footer.htm")	
	</div>
	<script src="$!webPath/resources/js/jquery-1.8.3.min.js"></script>
    <script src="$!webPath/resources/assets/bootstrap/js/bootstrap.min.js"></script>   
    <script src="$!webPath/resources/js/jquery.blockui.js"></script>
    <!-- ie8 fixes -->
    <!--[if lt IE 9]>
    <script src="$!webPath/resources/js/excanvas.js"></script>
    <script src="$!webPath/resources/js/respond.js"></script>
    <![endif]-->   
    <script src="$!webPath/resources/assets/uniform/jquery.uniform.min.js" type="text/javascript" ></script>
    <script src="$!webPath/resources/assets/chosen-bootstrap/chosen/v1.7/chosen.jquery.js" type="text/javascript"></script>
    <script src="$!webPath/resources/js/scripts.js"></script>
    <script src="$!webPath/resources/js/custom/page.js"></script>
    <script>
	jQuery(document).ready(function() { 
		App.init();
	});
	function printdiv(printId){  
		var headstr = "<html><head><title>工作量统计打印</title><link href='$!webPath/resources/css/custom/resume_print.css' rel='stylesheet' type='text/css'/></head><body>";  
		var footstr = "</body>";  
		var newstr = document.getElementById(printId).innerHTML;  
		var oldstr = document.body.innerHTML;  
		document.body.innerHTML = headstr+newstr+footstr;  
		window.print();  
		document.body.innerHTML = oldstr;  
		return false;  
	}  
	</script>
</body>
</html>