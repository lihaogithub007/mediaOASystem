<!DOCTYPE html>
<!--[if IE 8]> <html class="ie8"> <![endif]-->
<!--[if IE 9]> <html class="ie9"> <![endif]-->
<!--[if !IE]><!--> <html> <!--<![endif]-->
<head>
<meta charset="utf-8" />
<title>新媒体智能管理系统</title>
<meta content="width=device-width, initial-scale=1.0" name="viewport" />
<meta content="" name="description" />
<meta content="" name="author" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="renderer" content="webkit|ie-comp|ie-stand" />
<link href="$!webPath/resources/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
<link href="$!webPath/resources/assets/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet" />
<link href="$!webPath/resources/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
<link href="$!webPath/resources/css/style.css" rel="stylesheet" />
<link href="$!webPath/resources/css/style_responsive.css" rel="stylesheet" />
<link href="$!webPath/resources/css/style_default.css" rel="stylesheet" id="style_color" />
<link href="$!webPath/resources/assets/chosen-bootstrap/chosen/v1.7/chosen.css" rel="stylesheet" type="text/css" />
<link href="$!webPath/resources/assets/fancybox/source/jquery.fancybox.css" rel="stylesheet" />
<link href="$!webPath/resources/assets/toastr/toastr.css" rel="stylesheet" type="text/css" />
<link href="$!webPath/resources/assets/uniform/css/uniform.default.css" rel="stylesheet" type="text/css" />
<link href="$!webPath/resources/css/custom/custom.css" rel="stylesheet" type="text/css" />
<link href="$!webPath/resources/assets/My97DatePicker/skin/WdatePicker.css" rel="stylesheet" type="text/css"/>
</head>
<body class="fixed-top">
	<div id="header" class="navbar navbar-inverse navbar-fixed-top">
		$!httpInclude.include("/common/_header.htm") 
	</div>
	<div id="container" class="row-fluid">
		<div id="sidebar" class="nav-collapse collapse">
			$!httpInclude.include("/common/_sidebar.htm?op=cooperation_manage") 
		</div>
		<div id="main-content">
			<div class="container-fluid">
				<div class="row-fluid">
					<div class="span12">
						<h3 class="page-title">协同记录列表</h3>
						<ul class="breadcrumb">
							<li><a href="$!webPath/index.htm"><i class="icon-home"></i></a><span class="divider">&nbsp;</span></li>
					        <li><a href="javascript:void(0);">项目协同</a> <span class="divider">&nbsp;</span></li>
							<li><a href="$!webPath/admin/projectcoo/cooperation_list.htm">协同记录列表</a><span class="divider-last">&nbsp;</span></li>
						</ul>
					</div>
				</div>
				<div class="row-fluid">
	                <div class="span12">
	                    <div class="widget">
	                        <div class="widget-title">
	                            <h4><i class="icon-reorder"></i>协同记录列表</h4>
	                        </div>
	                        <div class="widget-body">
	                        	<form name="queryForm" id="queryForm" action="$!webPath/admin/projectcoo/cooperation_list.htm" method="post">
		                        	<div class="row-fluid">
										<div class="span12">
											 <div class="pull-left">
												<div class="ipt_container">
													<input type="text" name="project_name" id="project_name" value="$!{project_name}" class="search-ipt" placeholder="项目名称">
												</div>
												<div class="ipt_container">
													<input type="text" name="initiator_name" id="initiator_name" value="$!{initiator_name}" class="search-ipt" placeholder="发起人">
												</div>
												<div class="ipt_container">
													<input type="text" name="project_date" id="project_date" 
														value="$!project_date" class="search-ipt" placeholder="日期"
	                                        			onfocus="WdatePicker({dateFmt:'yyyy-MM-dd'})" readonly="readonly">
												</div>
                          						<input type="submit" class="btn btn-primary" value="搜索">
                          						<a href="$!webPath/admin/projectcoo/cooperation_add.htm?currentPage=$!currentPage" class="btn btn-primary">添加协同记录</a>
                          						<input type="button" class="btn btn-info" value="导出Excel" 
                          							onclick="exportExcel('queryForm','$!webPath/admin/projectcoo/cooperation_exportExcel.htm');">
												<div class="clearfix space7"></div>
											</div> 				
									  </div>
									</div>
								</form>
								<form name="listForm" id="listForm" action="$!webPath/admin/projectpoo/cooperation_list.htm" method="post">
		                        	#if(0!=$!{rows})
		                            <table class="table table-bordered table-hover">
		                                <thead>
			                                <tr>
			                               		<th style="width:5%;">序号</th>
			                               		<th style="width:8%;">日期</th>
			                               		<th style="width:10%;">项目名称</th>
			                               		<th style="width:10%;">发起人</th>
			                               		<th style="width:10%;">合作方</th>
			                               		<th style="width:20%;">结果</th>
			                               		<th style="width:20%;">备忘</th>
			                                    <th style="width:16%;">操作</th>
			                                </tr>
		                                </thead>
		                                <tbody>
		                                	#foreach($obj in $list)
			                                <tr>
			                                   <!--  <td>$!{velocityCount}</td> -->
			                                    <td>$!{velocityCount}</td>
			                                    <td>$!CommUtil.parseShortDateTime($!obj.project_date)</td>
			                                    <td>$!CommUtil.substring($!{obj.project_name}, 10)</td>
			                                    <td>$!CommUtil.substring($!{obj.initiator_name}, 10)</td>
			                                    <td>$!CommUtil.substring($!{obj.cooperation_name}, 10)</td>
			                                    <td>$!CommUtil.substring($!{obj.result}, 15)</td>
			                                    <td>$!CommUtil.substring($!{obj.remark}, 15)</td>
			                                    <td>
			                                        <a href="$!webPath/admin/projectcoo/cooperation_view.htm?id=$!{obj.id}&currentPage=$!currentPage">详情</a>&nbsp;
			                                        <a href="$!webPath/admin/projectcoo/cooperation_edit.htm?id=$!{obj.id}&currentPage=$!currentPage">编辑</a>&nbsp;
			                                    	<a href="$!webPath/admin/projectcoo/cooperation_delete.htm?id=$!{obj.id}&currentPage=$!currentPage"  
			                                    		onclick="return confirm('确认删除吗?')">删除</a>
			                                    </td>
			                                </tr>
			                                #end
		                                </tbody>
		                            </table>
		                            <div class="clearfix space5"></div>
		                            <div class="row-fluid">
									    <div class="span6">
									        <div class="dataTables_info" id="sample_1_info">总条数$!{rows}条</div>
									    </div>
									    <div class="span6">
									        <div class="dataTables_paginate paging_bootstrap pagination">
									        	$!{gotoPageFormHTML}
									        </div>
									    </div>
									</div>
									#else
									<hr class="clearfix" style="margin:10px 0px;">
									<span>没有协同记录</span>
									#end
									<input type="hidden" name="currentPage" id="currentPage" value="$!currentPage"/>
									<input type="hidden" name="dispatch_unit_name" value="$!dispatch_unit_name"/>
							  	</form>
	                        </div>
	                    </div>
	                </div>
				</div>
			</div>
		</div>
	</div>
	<div id="footer">
		$!httpInclude.include("/common/_footer.htm")	
	</div>
	<script src="$!webPath/resources/js/jquery-1.8.3.min.js"></script>
    <script src="$!webPath/resources/assets/bootstrap/js/bootstrap.min.js"></script>   
    <script src="$!webPath/resources/js/jquery.blockui.js"></script>
    <!-- ie8 fixes -->
    <!--[if lt IE 9]>
    <script src="$!webPath/resources/js/excanvas.js"></script>
    <script src="$!webPath/resources/js/respond.js"></script>
    <![endif]-->   
    <script src="$!webPath/resources/assets/uniform/jquery.uniform.min.js" type="text/javascript" ></script>
    <script src="$!webPath/resources/assets/chosen-bootstrap/chosen/v1.7/chosen.jquery.js" type="text/javascript"></script>
    <script src="$!webPath/resources/assets/toastr/toastr.js" type="text/javascript" ></script>
    <script src="$!webPath/resources/js/scripts.js"></script>
    <script src="$!webPath/resources/js/custom/page.js"></script>
    <script src="$!webPath/resources/js/custom/common.js"></script>
    <script src="$!webPath/resources/assets/My97DatePicker/WdatePicker.js" type="text/javascript" ></script>
    <script>
	jQuery(document).ready(function() {   
		App.init();
		initToastr();
	});
	function exportExcel(id, url) {
	    if(url != "")
			$("#" + id).attr("action", url);
		$("#" + id).submit();
			$("#" + id).attr("action", "$!webPath/admin/projectcoo/cooperation_list.htm");
	}
	</script>
</body>
</html>