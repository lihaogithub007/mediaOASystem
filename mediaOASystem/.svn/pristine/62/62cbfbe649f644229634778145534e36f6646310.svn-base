<script>
$(document).ready(function() {
	$("#sex").val("$!{user.userBase.sex}");
	$("#sex").trigger("chosen:updated");
	$("#department_id").val("$!{user.department.id}");
	$("#department_id").trigger("chosen:updated");
	$("#duty_id").val("$!{user.duty.id}"); 
	$("#duty_id").trigger("chosen:updated");
	$("#user_relationship").val("$!{user.user_relationship}"); 
	$("#user_relationship").trigger("chosen:updated");
	$("#blood_type").val("$!{user.userBase.blood_type}"); 
	$("#blood_type").trigger("chosen:updated");
	$("#constellation").val("$!{user.userBase.constellation}"); 
	$("#constellation").trigger("chosen:updated");
	$("#ID_type").val("$!{user.userBase.ID_type}"); 
	$("#ID_type").trigger("chosen:updated");
	$("#political_status").val("$!{user.userBase.political_status}"); 
	$("#political_status").trigger("chosen:updated");
	$("#marriage_status").val("$!{user.userBase.marriage_status}"); 
	$("#marriage_status").trigger("chosen:updated");
	$("#is_have_press_card").val("$!{user.userBase.is_have_press_card}"); 
	$("#is_have_press_card").trigger("chosen:updated");
	$("#domicile_type").val("$!{user.userBase.domicile_type}"); 
	$("#domicile_type").trigger("chosen:updated");
	App.init();
	jqueryValidate();
	bindChangeEvent();
	$("div.chosen-container.chosen-container-single").css("width","284");
});
function jqueryValidate(){
	jQuery.validator.addMethod("workYearsReg",function(value,element){
		var reg=/\d{1}|(^[1-9]\d{1}$)/;
		return this.optional(element) || (reg.test(value));
	}, "必须是0~100(不含)整数");
	
	jQuery("#baseForm").validate({
		errorElement : "span",
		errorPlacement : function(error, element) { 
			$(error).appendTo($(element).parent().siblings("div.err"));
		},
		onfocusout : function(element) {
	        $(element).valid();
	    },
		rules:{     
			true_name:{required:true},
			birthday:{required:true},
			department_id:{required:true},
			sex:{required:true},
			email:{email:true},
			home_postcode:{isZipCode:[]},
			present_postcode:{isZipCode:[]},
			personnel_file_postcode:{isZipCode:[]},
			emergency_contact_mobile:{isMobile:[]},
			work_years:{workYearsReg:[]},
			same_industry_work_years:{workYearsReg:[]},
       	},
		messages:{
			true_name:{required:"请输入用户姓名"},
      		birthday:{required:"请选择出生日期"},
      		department_id:{required:"请选择所属部门/科组"},
      		sex:{required:"请选择性别"},
      		
		}
	}); 
	var validator = $("#baseForm").data("validator");
    validator.settings.ignore = ":hidden:not(select)";
}
function bindChangeEvent(){
    jQuery("#department_id").change(function(){
        jQuery.ajax({
            type : "post",
            url : "$!webPath/admin/duty/ajaxQueryDutyUnderDepartment.htm",
            data : {//设置数据源
                department_id : jQuery(this).val()
            },
            dataType : "json",//设置需要返回的数据类型
            success : function(data) {
                if(data && data != null && data.result){
                    console.log(data.data);
                    jQuery("#duty_id").empty();  
                    jQuery("#duty_id").append("<option value=''>请选择岗位</option>");
                    jQuery.each(data.data, function(index, item){
                        var option = jQuery("<option>").val(item.id).text(item.duty_name);
                        jQuery("#duty_id").append(option);
                    });
                    jQuery("#duty_id").trigger("chosen:updated");
                }
            },
            error : function() {
                alert("系统异常，请稍后重试！");
            }
        });
    });
    //年龄
    $("#birthday").bind("blur",function(){
    	$("#age").val(getAge($(this).val()));
    });
    //工作年限
    $("#first_work_date").bind("blur",function(){
    	$("#work_years").val(getAge($(this).val()));
    });
}
</script>